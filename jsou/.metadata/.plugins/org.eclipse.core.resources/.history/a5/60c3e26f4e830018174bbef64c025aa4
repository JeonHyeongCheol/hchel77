package pack6;

public class VolumeMain {
	public static void main(String[] args) {
		//Volume volume = new Volume(); 추상, 인터페이스 이기 때문에 new 사용 불가.
		Volume volume = null;
		
		VolumeRadio radio = new VolumeRadio();
