package pack5;

public class Jepum {

	private int volume = 0;
	
	public Jepum() {
		System.out.println("추상 클래스 생성자");
	}
	
	public void setVolume(int volume) {
		this.volume = volume;
	}
	
	public int getVolume() {
		return volume;
	}
	
	public void volumeShow() {
		System.out.println("소리 크기는 " + volume);
	}
	
	public void volumeControl(); // body({})가 없는 메소드
	
}
