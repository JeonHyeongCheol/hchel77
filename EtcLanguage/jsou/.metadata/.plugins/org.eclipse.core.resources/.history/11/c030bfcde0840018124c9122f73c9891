package java_basic3;

import java.util.ArrayList;

public class ListTest {

	public static void main(String[] args) {
		// List Type의 Arraylist : 순서가 있고 중복을 허용 함.
		ArrayList<String> list = new ArrayList<>();
		list.add("kim");
		list.clear();
		list.add("kim");
		list.add("lee");
		list.add("kim");
		list.add("park");
		list.add("choi");
		
		System.out.println(list.size());
	}

}
